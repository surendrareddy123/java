public class usernumber
{
public static void main(String[] args)
{
Scanner input = new Scanner(System.in); System.out.println("enter usernumber:"); String S1 = input.nextLine(); System.out.println("reenter usernumber:"); String S2 = input.nextLine();
if (S1.equals(S2))
{

System.out.println("The user number is valid"); }
else
{
System.out.println("The user number is not valid");
}
}
}
